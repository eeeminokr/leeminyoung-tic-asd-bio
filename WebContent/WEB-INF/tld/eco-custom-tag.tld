<?xml version="1.0" encoding="UTF-8"?>
<taglib version="2.0" xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee web-jsptaglibrary_2_0.xsd">
	<tlib-version>1.0</tlib-version>
	<short-name>eco</short-name>
	<uri>www.ecoinsight.co.kr/tag</uri>

	<!-- 자바스크립트 파라미터 값을 만들어줌 -->
	<tag>
		<name>javascriptParameter</name>
		<tag-class>medical.framework.jstl.JavascriptParameterTag</tag-class>
		<body-content>jsp</body-content>
	</tag>

	<!-- 기준년월 - 년도 콤보 -->
	<tag>
		<name>yearCombo</name>
		<tag-class>medical.framework.jstl.YearComboTag</tag-class>
		<body-content>jsp</body-content>
		<attribute>
			<name>id</name>
			<required>false</required>
			<description>select tag id attribute. default "yyyy"</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>name</name>
			<required>false</required>
			<description>select tag name attribute. default "yyyy"</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<description>selected value</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>title</name>
			<required>false</required>
			<description>select title attriute. default "기준년"</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>allowBlank</name>
			<required>false</required>
			<description>allow blank (non select). default false</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>allowBlankText</name>
			<required>false</required>
			<description>allow blank text. default "전체"</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

	<!-- 기준년월 - 월 콤보 -->
	<tag>
		<name>monthCombo</name>
		<tag-class>medical.framework.jstl.MonthComboTag
		</tag-class>
		<body-content>jsp</body-content>
		<attribute>
			<name>id</name>
			<required>false</required>
			<description>select tag id attribute. default "mm"</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>name</name>
			<required>false</required>
			<description>select tag name attribute. default "mm"</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<description>selected value</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>title</name>
			<required>false</required>
			<description>select title attriute. default "기준월"</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>allowBlank</name>
			<required>false</required>
			<description>allow blank (non select). default true</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>allowBlankText</name>
			<required>false</required>
			<description>allow blank text. default "전체"</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<!-- 코드 category 를 기준으로 콤보 -->
	<tag>
		<name>codeCombo</name>
		<tag-class>medical.framework.jstl.CodeComboTag
		</tag-class>
		<body-content>jsp</body-content>
		<attribute>
			<name>id</name>
			<required>false</required>
			<description>select tag id attribute</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>name</name>
			<required>true</required>
			<description>select tag name attribute</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<description>selected value</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>title</name>
			<required>false</required>
			<description>select title attriute</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>classCd</name>
			<required>true</required>
			<description>code</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>allowBlank</name>
			<required>false</required>
			<description>allow blank (non select). default true</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>allowBlankText</name>
			<required>false</required>
			<description>allow blank text. default "전체"</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>showValue</name>
			<required>false</required>
			<description>show value option text. default false</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>showValuePrefix</name>
			<required>false</required>
			<description>show value option text prefix. default "["</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>showValueSuffix</name>
			<required>false</required>
			<description>show value option text suffix. default "]"</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
            <name>valueTextNo</name>
            <required>false</required>
            <description>set value option text, code_name or txt_value(s)</description>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
			<name>classCdDtlNmValue</name>
			<required>false</required>
			<description>set value classCdDtlNm. default false</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>performCntYN</name>
			<required>false</required>
			<description>set value performCntYN. default 'N'</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>check4Yn</name>
			<required>false</required>
			<description>set value check4Yn. default 'N'</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<!-- 권한 태그 -->
	<tag>
        <name>authorize</name> 
        <tagclass>medical.framework.jstl.AuthorizeTag</tagclass>
        <bodycontent>JSP</bodycontent> 
         <attribute> 
            <name>menuId</name> 
            <required>true</required> 
            <rtexprvalue>false</rtexprvalue> 
        </attribute>
         <attribute> 
            <name>authName</name> 
            <required>true</required> 
            <rtexprvalue>false</rtexprvalue> 
        </attribute>
    </tag>
    <!-- 사용자 권한 콤보 -->
	<tag>
		<name>grpAuthCombo</name>
		<tag-class>medical.framework.jstl.GrpAuthComboTag
		</tag-class>
		<body-content>jsp</body-content>
		<attribute>
			<name>id</name>
			<required>false</required>
			<description>select tag id attribute</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>name</name>
			<required>true</required>
			<description>select tag name attribute</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<description>selected value</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>title</name>
			<required>false</required>
			<description>select title attriute</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>allowBlank</name>
			<required>false</required>
			<description>allow blank (non select). default true</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>allowBlankText</name>
			<required>false</required>
			<description>allow blank text. default "전체"</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>showValue</name>
			<required>false</required>
			<description>show value option text. default false</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>showValuePrefix</name>
			<required>false</required>
			<description>show value option text prefix. default "["</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>showValueSuffix</name>
			<required>false</required>
			<description>show value option text suffix. default "]"</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
            <name>valueTextNo</name>
            <required>false</required>
            <description>set value option text, code_name or txt_value(s)</description>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
			<name>userAuthNmValue</name>
			<required>false</required>
			<description>set value classCdDtlNm. default false</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
</taglib>